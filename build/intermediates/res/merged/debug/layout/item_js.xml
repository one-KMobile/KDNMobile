<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llItemParent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tvCircuitName"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:text="서서울서수원#2"
            android:gravity="center" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <TextView
            android:id="@+id/tvCurrentLoad"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:gravity="center" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <TextView
            android:id="@+id/tvConductor_cnt"
            android:layout_width="40dp"
            android:gravity="center"
            android:layout_height="wrap_content" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <TextView
            android:id="@+id/tvLocation"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="전원-S" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <EditText
            android:id="@+id/editc1_js"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:imeOptions="flagNoExtractUi"
            android:inputType="numberDecimal|numberSigned"
            android:maxLength="5"
            android:layout_gravity="center"
            android:paddingLeft="8dp"
            android:background="@drawable/selector_input_s"
            android:nextFocusDown="@+id/editc1_jsj" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <EditText
            android:id="@+id/editc1_jsj"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:imeOptions="flagNoExtractUi"
            android:inputType="numberDecimal|numberSigned"
            android:maxLength="5"
             android:layout_gravity="center"
            android:paddingLeft="8dp"
            android:background="@drawable/selector_input_s"
            android:nextFocusDown="@+id/editc2_js" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <TextView
            android:id="@+id/tvc1_temp"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:maxLength="5" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <Button
            android:id="@+id/btnc1_yb_result"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_good_m"
            android:gravity="left|center_vertical"
            android:paddingLeft="10dp"
            android:text="조치후양호" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <EditText
            android:id="@+id/editc2_js"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:imeOptions="flagNoExtractUi"
            android:inputType="numberDecimal|numberSigned"
            android:maxLength="5"
             android:layout_gravity="center"
            android:paddingLeft="8dp"
            android:background="@drawable/selector_input_s"
            android:nextFocusDown="@+id/editc2_jsj" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <EditText
            android:id="@+id/editc2_jsj"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:imeOptions="flagNoExtractUi"
            android:inputType="numberDecimal|numberSigned"
            android:maxLength="5"
             android:layout_gravity="center"
            android:paddingLeft="8dp"
            android:background="@drawable/selector_input_s"
            android:nextFocusDown="@+id/editc3_js" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <TextView
            android:id="@+id/tvc2_temp"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:maxLength="5" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <Button
            android:id="@+id/btnc2_yb_result"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_good_m"
            android:gravity="left|center_vertical"
            android:paddingLeft="10dp"
            android:text="조치후양호" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <EditText
            android:id="@+id/editc3_js"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:imeOptions="flagNoExtractUi"
            android:inputType="numberDecimal|numberSigned"
            android:maxLength="5"
             android:layout_gravity="center"
            android:paddingLeft="8dp"
            android:background="@drawable/selector_input_s"
            android:nextFocusDown="@+id/editc3_jsj" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <EditText
            android:id="@+id/editc3_jsj"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:imeOptions="flagNoExtractUi"
            android:inputType="numberDecimal|numberSigned"
            android:layout_gravity="center"
            android:paddingLeft="8dp"
            android:background="@drawable/selector_input_s"
            android:maxLength="5" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <TextView
            android:id="@+id/tvc3_temp"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:maxLength="5" />

        <ImageView
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/table_line_h" />
        
        <Button
            android:id="@+id/btnc3_yb_result"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_good_m"
            android:gravity="left|center_vertical"
            android:paddingLeft="10dp"
            android:text="조치후양호" />
    </LinearLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/table_line_w" />
    
</LinearLayout>