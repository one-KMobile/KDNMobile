<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llItemParent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="36dp"
		android:orientation="horizontal">

		<TextView
			android:id="@+id/tvEqpNm"
			android:layout_width="290dp"
			android:layout_height="match_parent"
			android:gravity="center"
			android:text="서서울서수원#2" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<TextView
			android:id="@+id/tvMng01"
			android:layout_width="180dp"
			android:layout_height="wrap_content"
			android:gravity="center" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<TextView
			android:id="@+id/tvMng02"
			android:layout_width="80dp"
			android:layout_height="wrap_content"
			android:gravity="center" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<TextView
			android:id="@+id/tvUptlvlUplmt"
			android:layout_width="60dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:text="전원-S" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<TextView
			android:id="@+id/tvUptlvlLwlt"
			android:layout_width="60dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:maxLength="5" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<TextView
			android:id="@+id/tvUptlvlIntrcp"
			android:layout_width="60dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:maxLength="5" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<TextView
			android:id="@+id/tvSd"
			android:layout_width="40dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:maxLength="5" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<EditText
			android:id="@+id/editc1"
			android:layout_width="60dp"
			android:layout_height="match_parent"
			android:layout_gravity="center"
			android:background="@drawable/selector_input_s"
			android:imeOptions="flagNoExtractUi"
			android:inputType="numberDecimal|numberSigned"
			android:maxLength="5"
			android:nextFocusDown="@+id/editc2"
			android:paddingLeft="8dp" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<EditText
			android:id="@+id/editc2"
			android:layout_width="60dp"
			android:layout_height="match_parent"
			android:layout_gravity="center"
			android:background="@drawable/selector_input_s"
			android:imeOptions="flagNoExtractUi"
			android:inputType="numberDecimal|numberSigned"
			android:maxLength="5"
			android:nextFocusDown="@+id/editc3"
			android:paddingLeft="8dp" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<EditText
			android:id="@+id/editc3"
			android:layout_width="60dp"
			android:layout_height="match_parent"
			android:layout_gravity="center"
			android:background="@drawable/selector_input_s"
			android:imeOptions="flagNoExtractUi"
			android:inputType="numberDecimal|numberSigned"
			android:maxLength="5"
			android:nextFocusDown="@+id/editc4"
			android:paddingLeft="8dp" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:background="@drawable/table_line_h" />

		<EditText
			android:id="@+id/editc4"
			android:layout_width="60dp"
			android:layout_height="match_parent"
			android:layout_gravity="center"
			android:background="@drawable/selector_input_s"
			android:imeOptions="flagNoExtractUi"
			android:inputType="numberDecimal|numberSigned"
			android:maxLength="5"
			android:nextFocusDown="@+id/editc4"
			android:paddingLeft="8dp" />

	</LinearLayout>

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/table_line_w" />
    
</LinearLayout>